searchHotelName=SELECT BO.*   FROM (SELECT ROWNUM RNUM, B. * FROM (SELECT * FROM PETNER_MARKET WHERE MARKET_NAME LIKE '%' || ? || '%' \
		   OR MARKET_ADDRESS LIKE '%' || ? || '%'  OR MARKET_PLACE LIKE '%' || ? || '%' ORDER BY MARKET_ID DESC) B \
           WHERE ROWNUM <= ?) BO \
           WHERE RNUM >= ?




